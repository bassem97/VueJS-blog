{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": ["env.d.ts", "src/**/*", "src/**/*.vue"],
  "exclude": ["src/**/__tests__/*"],
  "compilerOptions": {
    "composite": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./*"],
        "@components/*": ["./src/components/*"],
        "@views/*": ["./src/views/*"],
        "@assets/*": ["./src/assets/*"],
        "@store/*": ["./src/store/*"],
        "@router/*": ["./src/router/*"],

    }
  }
}
